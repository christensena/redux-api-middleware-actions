{
  "name": "redux-typed-async-actions",
  "version": "0.0.1",
  "description": "Strongly typed async Redux actions via api middleware",
  "main": "index.js",
  "typings": "typings/index.d.ts",
  "author": "Alan Christensen <alan@christensen.org.nz>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib typings && tsc",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "files": [
    "index.js",
    "lib",
    "typings"
  ],
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "jest": "^19.0.2",
    "redux-api-middleware": "^1.0.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.14",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {},
  "keywords": [
    "typescript",
    "redux",
    "middleware",
    "async"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
